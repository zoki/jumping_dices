{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "impliedStrict": true,
      "jsx": false
    }
  },
  "settings": {},
  "rules": {
    "class-methods-use-this": 0,
    "max-len": 0,
    "no-new": 0,
    "no-return-assign": 0,
    "no-continue": 0,
    "no-plusplus": 0,
    "no-unused-expressions": [2, {
      "allowShortCircuit": true,
      "allowTernary": true
    }],
   "lines-between-class-members": ["error", "always", {
     "exceptAfterSingleLine": true
   }],
   "no-console": "off",
   "no-underscore-dangle": [
      "error", {
        "allow": ["_id", "__private__"]
      }
    ]
  }
}
